<?xml version="1.0" encoding="UTF-8"?>
<jyro xmlns="http://www.koiroha.org/xmlns/jyro/configuration_1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.koiroha.org/xmlns/jyro/configuration_1.0 jyro.xsd">
	<property name="a.b.c" value="foo"/>
	<property name="classpath" value="/var/lib/java"/>

	<queue id="hoge">
		<jms factory-jndi-name="" />
	</queue>

	<node id="URL取得Java" queue="hoge" classpath="${classpath}/hoge.jar">
		<worker min="5" max="10" class="org.koiroha.jyro.SampleWorker"/>
	</node>

	<node id="URL取得Script" queue="hoge" classpath="${classpath}/hoge.jar">
		<script min="5" max="10" type="application/javascript" includes="scripts/**/*.js">
		</script>
	</node>

</jyro>
